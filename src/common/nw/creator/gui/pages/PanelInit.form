<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="common.nw.creator.gui.pages.PanelInit">
  <grid id="27dc6" binding="panel_init" layout-manager="GridBagLayout">
